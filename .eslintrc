{
    "extends": ["react-app"],
    "rules": {
        "semi": [
            "warn",
            "always"
        ],
        "indent": [
            "error",
            2,
            {
                "SwitchCase": 1
            }
        ],
        "object-curly-spacing": [
            "error",
            "never"
        ],
        "no-use-before-define": 0
    }
}